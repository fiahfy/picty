{
  "name": "next-explorer",
  "description": "WIP",
  "version": "0.0.0",
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "date-fns": "^2.28.0",
    "electron-context-menu": "^3.1.2",
    "electron-is-dev": "^2.0.0",
    "electron-next": "^3.1.5",
    "electron-window-state": "^5.0.3",
    "file-url": "^4.0.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-virtualized": "^9.22.3"
  },
  "devDependencies": {
    "@fiahfy/eslint-config-next": "^0.0.16",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-virtualized": "^9.21.20",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "main": "main/index.js",
  "private": true,
  "productName": "next-explorer",
  "scripts": {
    "build": "npm run build-renderer && npm run build-electron",
    "build-electron": "tsc -p electron-src",
    "build-renderer": "next build renderer && next export renderer",
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:prettier": "prettier -c --ignore-path .gitignore .",
    "package": "npm run build && electron-builder",
    "package-dir": "npm run build && electron-builder --dir",
    "prepare": "husky install",
    "release": "npm run build && electron-builder -mlw -p",
    "test": "npm run lint"
  }
}
